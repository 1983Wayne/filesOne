;;; ( SIMPLY SCHEME EXERCISE 12.9 ) Make a recursive, real-word saving procedure with a sentence as an argument.

(define (real-wd? wd)
  (not (member? wd '(a the an in of and for to with))))

(define (real-words sent)
  (cond ((equal? (count sent) 0) '())
        ((real-wd? (first sent)) (se (first sent) (real-words (bf sent))))
        (else (real-words (bf sent)))))
